			reader.readAsDataURL(files[0]); // read the local file
			reader.onloadend = function(){ // set image data as background of div
				document.getElementById('photo-preview5').style.display = "block";
				document.getElementById('img-preview5').src=this.result;
				document.getElementById('action-preview5').style.display = "block";
			}
		}
	});
	
	$("#cancel-upload5").click(function() {
		var r_confirm = confirm('Apakah yakin akan menghapus foto ini? \nSetelah dihapus, foto tidak dapat dikembalikan.');
		if (r_confirm == true){
			if (document.getElementById('foto5').value != ""){
				document.getElementById('delete_foto5').value = document.getElementById('foto5').value;
			}
			document.getElementById('img-preview5').src = "";
			document.getElementById('photo5').value = "";
			document.getElementById('foto5').value = "";
			document.getElementById('photo-preview5').style.display = "none";
			document.getElementById('action-preview5').style.display = "none";
		} else {
			return false;
		}
	});
		
	$("#remove-file").click(function() {
		var r_confirm = confirm('Apakah yakin akan menghapus file ini? \nSetelah dihapus, file tidak dapat dikembalikan.');
		if (r_confirm == true){
			if (document.getElementById('nama_file').value != ""){
				document.getElementById('delete_file').value = document.getElementById('nama_file').value;
			}
			document.getElementById("preview-file-btl").innerHTML = "Tidak ada file.";
			document.getElementById("preview-file-bl").innerHTML = "Tidak ada file.";
		} else {
			return false;
		}
	});
	
	$("#btn-save-coord").click(function() {
		document.getElementById('latlong').value = document.getElementById('latitudelongitude').value;
	});
	
	$('.photo-preview img, .photo-detail img').imgCentering({
		'forceSmart': true		//centering without showing background
	});
});

</script>


